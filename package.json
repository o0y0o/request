{
  "name": "request",
  "description": "Easy-to-fetch HTTP client for the browser and NodeJS.",
  "repository": "https://github.com/o0y0o/request",
  "author": "Jason Chung <shiningjason1989@gmail.com>",
  "license": "MIT",
  "private": true,
  "keywords": [
    "fetch",
    "http",
    "https",
    "http-client",
    "http-request",
    "network",
    "request",
    "web",
    "browser",
    "nodejs"
  ],
  "scripts": {
    "format": "prettier --write '{*,src/**/*}.js'",
    "lint": "eslint '{*,src/**/*}.js'",
    "test": "BABEL_ENV=test mocha -r @babel/register src/**/*.test.js",
    "build": "rollup -c",
    "pkg:node": "cd ./packages/node && yarn publish",
    "pkg:web": "cd ./packages/web && yarn publish",
    "pkg": "yarn pkg:node & yarn pkg:web & wait"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.10.5",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-inject": "^4.0.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "fetch-mock": "^9.10.7",
    "form-data": "^3.0.0",
    "mocha": "^8.1.3",
    "node-fetch": "^2.6.0",
    "prettier": "^2.1.1",
    "rollup": "^2.26.8",
    "rollup-plugin-terser": "^7.0.1"
  }
}
