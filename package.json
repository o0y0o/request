{
  "name": "request",
  "description": "Easy-to-fetch HTTP client for the browser and NodeJS.",
  "repository": "https://github.com/o0y0o/request",
  "author": "Jason Chung <shiningjason1989@gmail.com>",
  "license": "MIT",
  "private": true,
  "keywords": [
    "fetch",
    "http",
    "https",
    "http-client",
    "http-request",
    "network",
    "request",
    "web",
    "browser",
    "nodejs"
  ],
  "scripts": {
    "ncu": "ncu && lerna exec ncu",
    "format": "prettier --write '{*,src/**/*}.js'",
    "lint": "eslint '{*,src/**/*}.js'",
    "test": "BABEL_ENV=test mocha -r @babel/register src/**/*.test.js",
    "build": "rollup -c",
    "ver": "lerna version --no-push && git tag | xargs git tag -d",
    "pkg": "lerna publish from-package --yes"
  },
  "devDependencies": {
    "@0y0/eslint-config-vanilla": "^1.1.2",
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/register": "^7.12.10",
    "@rollup/plugin-babel": "^5.2.3",
    "@rollup/plugin-inject": "^4.0.2",
    "eslint": "^7.18.0",
    "fetch-mock": "^9.11.0",
    "form-data": "^3.0.0",
    "lerna": "^3.22.1",
    "mocha": "^8.2.1",
    "node-fetch": "^2.6.1",
    "prettier": "^2.2.1",
    "rollup": "^2.38.1",
    "rollup-plugin-terser": "^7.0.2"
  }
}
